{
  "name": "server",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "ts-node src/server.ts",
    "dev": "nodemon --exec ts-node src/server.ts",
    "migration:create": "ts-node ./../node_modules/typeorm/cli.js migration:create",
    "migration:generate": "ts-node ./../node_modules/typeorm/cli.js migration:generate",
    "migration:run": "ts-node ./../node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./../node_modules/typeorm/cli.js migration:revert",
    "seed": "ts-node ./../node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.10",
    "@types/sinon": "7.0.13",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-testing-library": "^3.10.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "body-parser": "^1.18.1",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "minifaker": "^1.34.0",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^4.0.3",
    "typeorm": "0.2.38",
    "typeorm-seeding": "^1.6.1",
    "winston": "^3.3.3",
    "mocha": "6.1.4",
    "sinon": "7.3.2"
  }
}
